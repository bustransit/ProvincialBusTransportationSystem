	<queryString>
		<![CDATA[SELECT
     training_participants.`rec_id` AS training_participants_rec_id,
     training_participants.`training_id` AS training_participants_training_id,
     training_participants.`emp_id` AS training_participants_emp_id,
     training_assessment.`record_id` AS training_assessment_record_id,
     training_assessment.`training_id` AS training_assessment_training_id,
     training.`training_id` AS training_training_id,
     training.`title` AS training_title,
     training.`description` AS training_description,
     training.`participants` AS training_participants,
     training.`time` AS training_time,
     training.`end_time` AS training_end_time,
     training.`target_date` AS training_target_date,
     training.`venue` AS training_venue,
     training.`type` AS training_type,
     department.`department_code` AS department_department_code,
     department.`department_name` AS department_department_name,
     department.`description` AS department_description,
     department.`scope` AS department_scope,
     employee.`emp_id` AS employee_emp_id,
     employee.`firstname` AS employee_firstname,
     employee.`middlename` AS employee_middlename,
     employee.`lastname` AS employee_lastname,
     employee.`birthdate` AS employee_birthdate,
     employee.`address` AS employee_address,
     employee.`contact_info` AS employee_contact_info,
     employee.`immediate_supervisor_code` AS employee_immediate_supervisor_code,
     employee.`age` AS employee_age,
     employee.`gender` AS employee_gender,
     employee.`position_id` AS employee_position_id,
     employee.`sss` AS employee_sss,
     employee.`bir` AS employee_bir,
     employee.`tax` AS employee_tax,
     employee.`philhealth` AS employee_philhealth,
     employee.`hdmf` AS employee_hdmf,
     employee.`hiring_req_id` AS employee_hiring_req_id,
     employee.`education_rec_id` AS employee_education_rec_id,
     employee.`clearances_rec_id` AS employee_clearances_rec_id,
     employee.`employment_history_rec_id` AS employee_employment_history_rec_id,
     employee_position.`position_id` AS employee_position_position_id,
     employee_position.`position_name` AS employee_position_position_name,
     employee_position.`description` AS employee_position_description,
     employee_position.`scope` AS employee_position_scope,
     employee_position.`population` AS employee_position_population,
     employee_position.`vacancy` AS employee_position_vacancy,
     employee_position.`department_code` AS employee_position_department_code,
     employee_position.`position_level` AS employee_position_position_level
FROM
     `department` department INNER JOIN `employee_position` employee_position ON department.`department_code` = employee_position.`department_code`
     INNER JOIN `employee` employee ON employee_position.`position_id` = employee.`position_id`,
     `training_participants` training_participants,
     `training_assessment` training_assessment,
     `training` training]]>
	</queryString>
